{
	"extends": "./path.json",
	"compilerOptions": {
		"module": "commonjs",
		"target": "ES6",
		"outDir": "../build/src",

		"declaration": false,
		"sourceMap": false,
		"inlineSourceMap": false,

    "useDefineForClassFields": true,
    "esModuleInterop": true,
		"strict": true,
    "typeRoots": [
      "./node_modules/@types",
      "./src/types",
    ]
	},
	"include": [
		"src"
	],
	"exclude": [
		"node_modules"
	]
}